<config>
  <rule>
    <name>module maven java bytecode</name>
    <tag>specific</tag>
    <match>
      <rule>module maven</rule>
    </match>
    
    <java-bytecode>
      <int-range>49 - 52</int-range>
    </java-bytecode>
  </rule>
  
  <rule>
    <name>module maven filesize</name>
    <tag>specific</tag>
    <match>
      <rule>module maven</rule>
    </match>
    
    <rpm-file-size-bytes>
      <int-range>- 5'500'000</int-range>
    </rpm-file-size-bytes>
  </rule>
  
  <rule>
    <name>module maven provides</name>
    <tag>specific</tag>
    <match>
      <rule>module maven</rule>
    </match>
    
    <provides>
      <any>
        <regex>.*ant-.*</regex>
        <regex>.*apache.commons.*</regex>
        <regex>.*apache-parent.*</regex>
