        <regex>.*xbean.*</regex>
        <regex>.*xmlunit.*</regex>
        <regex>.*xmvn.*</regex>
        <regex>.*xz.*</regex>
        
        <regex>.*eclipse.*</regex>
        <regex>.*javax.*</regex>
        <regex>.*commons.*</regex>
        <regex>.*classworlds.*</regex>
      </any>
    </provides>
  </rule>
  
  <rule>
    <name>module maven requires</name>
    <tag>specific</tag>
    <match>
      <rule>module maven</rule>
    </match>
    
    <requires>
      <any>
        <text>alternatives</text>
        
        <regex>.*ant-.*</regex>
        <regex>.*maven.*</regex>
        <regex>.*apache.*</regex>
        <regex>.*javapackages.*</regex>
        <regex>.*xmvn.*</regex>
